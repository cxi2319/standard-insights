{
  "$id": "reviews-keyword-sentiment-keyword-bar",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Keyword Sentiment by Review Keywords",
  "description": "Keyword Sentiment by Review Keywords",
  "query": {
    "metrics": [
      "yext/TOPIC_SENTIMENT"
    ],
    "dimensions": [
      "yext/REVIEW_TOPICS"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "WEEKS_12"
      }
    ]
  },
  "display": {
    "bar": {}
  }
}