{
    "$id": "demo-answered-calls-month-prev-bar",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Demo Answered Calls by Month",
    "description": "Demo Answered Calls by Month",
    "query": {
      "metrics": [
        "yext/DEMO_ANSWERED_CALLS"
      ],
      "dimensions": [
        "yext/MONTHS"
      ],
      "filters": [
        {
          "filter": "yext/date",
          "in": "MONTHS_4"
        }
      ],
      "comparison": "PREVIOUS_PERIOD"
    },
    "display": {
      "bar": {}
    }
  }