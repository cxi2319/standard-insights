{
    "$id": "listings-listings-impressions-month-app-line",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Listings Impressions by Month and App",
    "description": "Listings Impressions by Month and App",
    "query": {
      "metrics": [
        "yext/LISTINGS_IMPRESSIONS"
      ],
      "dimensions": [
        "yext/MONTHS",
        "yext/APP"
      ],
      "filters": [
        {
          "filter": "yext/date",
          "in": "MONTHS_4"
        }
      ]
    },
    "display": {
      "line": {
        "area": true
      }
    }
  }